    </route>
    <route>
        <pattern>/{organisationid:\d+}/person/{personid:\d+}</pattern>
        <generated-parameter name="mvcPath">/html/iris/irislinkedopendata/rdf-foaf-person.jsp</generated-parameter>
        <implicit-parameter name="p_p_state">exclusive</implicit-parameter>
    </route> 
    <route>
        <pattern>/{organisationid:\d+}/diseasesMatrix/{diseasematrixid:\d+}</pattern>
        <generated-parameter name="mvcPath">/html/iris/irislinkedopendata/rdf-miabis-disease.jsp</generated-parameter>
